<instruction text="&#34;Affecter 0 à values.modeAngle&#34;" comment="" color="FFFFFF" rotated="0"></instruction>
 </qFalse>
</alternative>
<instruction text="&#34;Affecter 1 à values.mode&#34;" comment="" color="FFFFFF" rotated="0"></instruction>
<call text="&#34;lectureCntIterations(&#38;cntBin, &#38;cntTrigo)&#34;" comment="" color="FFFFFF"></call>
